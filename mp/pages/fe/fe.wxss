/*header*/
.wrap-user-info {
  margin: 40rpx 20rpx 0;
  padding: 20rpx;
  box-shadow: 0 0 2px 2px #e9e9e9;
  border-radius: 2px;
}

.user-info {
  display: flex;
  justify-content: center;
}

.userinfo-avatar {
  width: 96rpx;
  height: 96rpx;
}

.wrap-userinfo {
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  align-content: space-around;
}

.userinfo-name {
  font-size: 16px;
  color: #333;
}

.userinfo-des {
  margin-top: 16rpx;
  font-size: 12px;
  color: #9d9d9d;
}

/*menu*/
.fe-list {
  margin: 0 20rpx;
  padding-top: 20rpx;
}

.fe-list .fe-item {
  margin: 20rpx 0;
  box-shadow: 0 0 2px 2px #e9e9e9;
  border-radius: 2px;
}

.fe-item .item {
  display: flex;
  justify-content: space-between;
  padding: 15rpx 30rpx 15rpx 60rpx;
  font-size: 14px;
  color: #24BDDF;
}

