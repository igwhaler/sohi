{
  "name": "@bit/bit.envs.compilers.react",
  "version": "1.0.18",
  "homepage": "https://bit.dev/bit/envs/compilers/react",
  "main": "dist/react/index.js",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-export-default-from": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.7",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "vinyl": "^2.2.0",
    "chai": "4.2.0",
    "rimraf": "3.0.0",
    "@babel/core": "^7.8.4"
  },
  "devDependencies": {
    "@types/vinyl": "^2.0.3",
    "@types/node": "^12.7.4",
    "@types/mocha": "^5.2.7",
    "bit-bin": "^14.7.1"
  },
  "peerDependencies": {},
  "license": "SEE LICENSE IN LICENSE",
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/typescript@3.1.46": {
          "rawConfig": {
            "compilerPath": "typescript/bin/tsc",
            "compilerArguments": [
              "--declaration"
            ],
            "compiledFileTypes": [
              ".ts",
              ".tsx"
            ],
            "configFileName": "tsconfig.json",
            "tsconfig": {
              "compilerOptions": {
                "outDir": "dist",
                "target": "ES2015",
                "sourceMap": false,
                "moduleResolution": "Node",
                "esModuleInterop": true,
                "module": "CommonJS",
                "allowSyntheticDefaultImports": true,
                "resolveJsonModule": true,
                "declaration": true,
                "experimentalDecorators": true,
                "inlineSourceMap": false,
                "inlineSources": false,
                "jsx": "react",
                "rootDir": "./",
                "removeComments": true,
                "typeRoots": [
                  "./node_modules/@types"
                ],
                "importHelpers": false,
                "skipLibCheck": true
              },
              "include": [
                "./**/*"
              ],
              "exclude": [
                "node_modules",
                "dist",
                ".dependencies",
                "*.spec.ts",
                "*.e2e.ts"
              ]
            },
            "development": false,
            "copyPolicy": {
              "ignorePatterns": [
                "package.json",
                "package-lock.json",
                "tsconfig.json"
              ],
              "disable": false
            }
          },
          "options": {},
          "files": {}
        }
      }
    },
    "overrides": {
      "dependencies": {
        "@babel/core": "^7.8.4",
        "bit-bin": "-"
      },
      "devDependencies": {
        "@types/node": "+",
        "@types/mocha": "^5.2.7",
        "bit-bin": "^14.7.1"
      }
    }
  }
}
